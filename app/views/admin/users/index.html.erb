<div class = "container">
  <div class = "row">
    <div class = "col mt-4 mb-4">
      <h2>ユーザー一覧</h2>
    </div>
  </div>
    <div class="row">
      <% @users.each do |user| %>
      <div class="col-sm-3">
        <%= image_tag user.get_profile_image(100, 100) %><br>
        <%= link_to user.name,admin_user_path(user.id)%>
      </div>
      <% end %>
    </div>
  </div>
</div>